<main role="main" class="inner cover">
  <h1 class="cover-heading">Latest Tweets on Ruby on Rails</h1>
  <div class="row">
    <% @tweets.each do |tweet| %>
      <div class="col-md-6">
        <div class="tech-news">
          <div class="card">
              <%=twitter_parser tweet %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</main>
